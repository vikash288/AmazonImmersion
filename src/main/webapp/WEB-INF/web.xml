<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RedShiftWebServices</servlet-name>
  	<display-name>RedShiftWebServices</display-name>
  	<description></description>
  	<servlet-class>com.amazonbyod.webservices.RedShiftWebServices</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Dashboard</servlet-name>
  	<display-name>Dashboard</display-name>
  	<description></description>
  	<servlet-class>com.amazonbyod.servlet.Dashboard</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DashboardOperation</servlet-name>
  	<display-name>DashboardOperation</display-name>
  	<description></description>
  	<servlet-class>com.amazonbyod.servlet.DashboardOperation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProperties</servlet-name>
  	<display-name>EditProperties</display-name>
  	<description></description>
  	<servlet-class>com.amazonbyod.servlet.EditProperties</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RedShiftWebServices</servlet-name>
  	<url-pattern>/RedShiftWebServices</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Dashboard</servlet-name>
  	<url-pattern>/Dashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DashboardOperation</servlet-name>
  	<url-pattern>/DashboardOperation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProperties</servlet-name>
  	<url-pattern>/EditProperties</url-pattern>
  </servlet-mapping>
</web-app>
